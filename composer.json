{
    "name": "andydune/mongo-query",
    "description": "Add beauty to momgodb query arrays.",
    "license": "MIT",
    "keywords": [
        "php",
        "mongo",
        "query-builder"
    ],
    "authors": [
        {
            "name": "Andrey Ryzhov",
            "email": "info@rznw.ru"
        }
    ],
    "homepage": "https://github.com/AndyDune/Pipeline",
    "autoload": {
        "psr-4": {
            "AndyDune\\MongoQuery\\": "src/"
        }
    },
    "require": {
        "php": "^7.0 || ^7.1",
        "mongodb": ">=2.6.0",
        "mongodb/mongodb": "^1.1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.15 || ^6.0.8",
        "mongodb/mongodb": "^1.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": "true"
    },
    "autoload-dev": {
        "psr-4": {
            "AndyDune\\MongoQueryTest\\": "test/"
        }
    }
}